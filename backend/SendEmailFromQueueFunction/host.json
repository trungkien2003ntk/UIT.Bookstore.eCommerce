{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:01:00",
      "visibilityTimeout": "00:00:20",
      "batchSize": 16,
      "maxDequeueCount": 3,
      "newBatchThreshold": 8,
      "messageEncoding": "base64"
    }
  }
}